$('#<%= params[:dom_id] %>').replaceWith('<%= j(render 'votes/button', poll: @poll, class_name: params[:class_name], counter_dom_id: params[:counter_dom_id]) %>');
<% if params[:counter_dom_id].present? %>
  $('#<%= params[:counter_dom_id] %>').replaceWith('<%= j(render 'votes/counter', poll: @poll, counter_dom_id: params[:counter_dom_id]) %>');
<% end %>
